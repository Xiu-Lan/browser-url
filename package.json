{
  "name": "browser-url",
  "version": "1.3.3",
  "description": "URL utility for browser",
  "main": "dist/Url.js",
  "scripts": {
    "build": "babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/browser-url.git"
  },
  "keywords": [
    "url"
  ],
  "author": "Jiang Fengming <fenix@noindoin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/browser-url/issues"
  },
  "homepage": "https://github.com/fenivana/browser-url#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-enough": "^0.2.8"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": "umd"
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
